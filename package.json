{
  "name": "football-platform",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build-dev && npm run build-prod",
    "deploy-dev": "npm run build-dev && npm run deploy-hosting-dev",
    "deploy-prod": "npm run build-prod && npm run deploy-hosting-prod",
    "lint-functions": "npx eslint --fix 'functions/src'",
    "deploy-functions-dev": "firebase use football-platform-dev && npm run deploy-functions",
    "deploy-functions-prod": "firebase use football-platform-production && npm run deploy-functions",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build-prod": "ng build --configuration production",
    "build-dev": "ng build --configuration development",
    "deploy-functions": "npm run lint-functions && firebase deploy --only functions",
    "deploy-hosting-prod": "npm run build-prod && firebase use football-platform-production && firebase deploy --only hosting -P football-platform-production",
    "deploy-hosting-dev": "firebase use football-platform-dev && firebase deploy --only hosting -P football-platform-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0-next.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.0.0-next.0",
    "@angular/compiler": "~13.0.0-next.0",
    "@angular/core": "~13.0.0-next.0",
    "@angular/fire": "^7.2.1",
    "@angular/forms": "~13.0.0-next.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.0.0-next.0",
    "@angular/platform-browser-dynamic": "~13.0.0-next.0",
    "@angular/router": "~13.0.0-next.0",
    "@ballzo-ui/core": "^1.0.26",
    "firebase": "^10.4.0",
    "ngx-image-compress": "^13.1.16",
    "rxfire": "^6.0.4",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.0.0",
    "@angular/compiler-cli": "~13.0.0-next.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  }
}
